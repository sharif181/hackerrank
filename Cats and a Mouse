#include<bits/stdc++.h>
using namespace std;

int main()
{
    int T,x,y,z,a,b;
    cin>>T;
    while(T--){
        cin>>x>>y>>z;
        a=abs(x-z);
        b=abs(y-z);
        if(a==b) cout<<"Mouse C"<<endl;
        else if(a>b)cout<<"Cat B"<<endl;
        else cout<<"Cat A"<<endl;
    }
    return 0;
}


